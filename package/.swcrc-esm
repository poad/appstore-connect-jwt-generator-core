{
  "$schema": "http://json.schemastore.org/swcrc",
  "test": ".*/*.(m)?ts$",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "target": "es2021"
  },
  "minify": false,
  "module": {
    "type": "es6"
  },
  "sourceMaps": true,
  "exclude": [
    "tests",
    ".*.js$",
    ".*.map$"
  ]
}